<UserControl x:Class="HandyControlDemo.UserControl.ImageBrowserDemoCtl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Background="White"
             xmlns:langs="clr-namespace:HandyControlDemo.Properties.Langs"
             DataContext="{Binding ImageBrowserDemo,Source={StaticResource Locator}}"
             xmlns:controls="clr-namespace:HandyControl.Controls;assembly=HandyControl">
    <controls:TransitioningContentControl>
        <Button Command="{Binding OpenImgCmd}" Content="{x:Static langs:Lang.OpenImageBrowser}" Style="{StaticResource ButtonPrimary}" Margin="32"></Button>
    </controls:TransitioningContentControl>
</UserControl>
